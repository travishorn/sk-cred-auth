{
	"name": "sk-cred-auth",
	"version": "0.0.1",
	"description": "A basic example of a SvelteKit app with authentication based on username and password.",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"keywords": [
		"svelte",
		"sveltekit",
		"credential",
		"authentication"
	],
	"author": "Travis Horn <travis@travishorn.com> (https://travishorn.com/)",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/travishorn/sk-cred-auth.git"
	},
	"bugs": {
		"url": "https://github.com/travishorn/sk-cred-auth/issues"
	},
	"homepage": "https://github.com/travishorn/sk-cred-auth#readme",
	"type": "module",
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.20.4",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.30.0",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"svelte": "^4.0.5",
		"svelte-check": "^3.4.3",
		"typescript": "^5.0.0",
		"vite": "^4.4.2"
	},
	"dependencies": {
		"argon2": "^0.30.3",
		"better-sqlite3": "^8.5.0",
		"cookie": "^0.5.0",
		"knex": "^2.5.1"
	}
